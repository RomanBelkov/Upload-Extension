        <TextBox Name="TargetIP" Margin="10,10,10,48" FontSize="16" TextChanged="TargetIP_TextChanged">New TRIK IP</TextBox>
        <Button Margin="10,49,0,8" Click="Button_Click" IsDefault="True" HorizontalAlignment="Left" Width="91">OK</Button>
        <Button Margin="119,49,10,8" Content="Cancel" IsCancel="True" />
    </Grid>
</Window>