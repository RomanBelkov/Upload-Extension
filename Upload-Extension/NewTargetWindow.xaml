<Window x:Class="Trik.Upload_Extension.NewTargetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NewTargetWindow" Height="121" Width="226" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox Name="TargetIP" Margin="10,10,10,48" FontSize="16" TextChanged="TargetIP_TextChanged">New TRIK IP</TextBox>
        <Button Margin="0,47,10,10" HorizontalAlignment="Right" Width="91" Click="Button_Click" IsDefault="True">OK</Button>
        <Button Margin="10,47,0,10" HorizontalAlignment="Left" Width="91" Content="Cancel" IsCancel="True" />
    </Grid>
</Window>